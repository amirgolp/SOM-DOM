FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Surf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -2 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }

    porewalls
    {
        type          reactingWall;
        surfaceMasters
        {
            Surf_w
            {
	        density 2.4e-9;
            };
        };
        value uniform 2.4e-9;
    }

    inlet
    {
        type            zeroGradient;
    }
	
    outlet
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type 		empty;
    }

    movingWalls_patch0
    {
        type          reactingWall;
        surfaceMasters
        {
            Surf_w
            {
	        density 2.4e-9;
            };
        };
        value uniform 2.4e-9;
    }

}

