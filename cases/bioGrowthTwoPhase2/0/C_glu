FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      C_glu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0.2;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }

    porewalls
    {
        type            zeroGradient;
    }

    inlet
    {
        type            zeroGradient;//fixedValue;
        value           uniform 1;
    }
	
    outlet
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type 		empty;
    }

    movingWalls_patch0
    {
        type            zeroGradient;
    }
}

